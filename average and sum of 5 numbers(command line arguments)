class SumAverage {
    public static void main(String[] args) {
        int sum = 0;

        for (int i = 0; i < 5; i++) {
            sum = sum + Integer.parseInt(args[i]);
        }

        double average = sum / 5.0;

        System.out.println("Sum = " + sum);
        System.out.println("Average = " + average);
    }
}
